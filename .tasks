[build]
command=cd build && cmake -DCMAKE_EXPORT_COMPILE_COMMANDS=1 .. && make -j10
cwd=$(VIM_ROOT)

[execute]
command=cd build && cmake -DCMAKE_EXPORT_COMPILE_COMMANDS=1 .. && make -j10 && renderer.app/Contents/MacOS/renderer
cwd=$(VIM_ROOT)

[test]
command=cd build && cmake -DCMAKE_EXPORT_COMPILE_COMMANDS=1 .. && make -j10 && make test
cwd=$(VIM_ROOT)
